rows = int(input("Enter the number of rows: "))
columns = int(input("Enter the number of columns: "))
matrix = []

print("Enter the entries row-wise:")
for i in range(rows):
    row = []
    for j in range(columns):
        row.append(int(input()))
    matrix.append(row)

print("Matrix:")
for row in matrix:
    for val in row:
        print(val, end=" ")
    print()
