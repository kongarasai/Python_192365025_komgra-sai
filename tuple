t1=(90,12,45,5.6)
t2=(87,5.6,5,4)
t3=sorted(t1+t2)
print("Maximum: ", t3[len(t3)-1])
print("Minimum: ", t3[0])
print("Sum: ", t1+t2)
t3=set(t1+t2)
print(t3)
t3=tuple(t3)
print(t3[1:6])
print(t3[:8])
print(t3[::-1])
t4=list(t3)
print("List from a tuple: ", t4)
if t1>t2:
    print(t1, "is greater")
elif(t2>t1):
    print(t2, "is greater")
else:
    print(t1,t2, "have equal no.of elements")
l=("hi",4.5,3)
m=(90,"hello",4,8)
print(l, end=' ')
print(m)
